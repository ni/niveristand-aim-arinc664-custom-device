	<xs:element name="DynamicData">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DynamicByte" maxOccurs="4">
					<xs:complexType>
						<xs:attribute name="function" use="required">
							<xs:annotation>
								<xs:documentation>Function to be performed on the byte specified.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="increment"/>
									<xs:enumeration value="decrement"/>
									<xs:enumeration value="sawtooth"/>
									<xs:enumeration value="shift-left"/>
									<xs:enumeration value="shift-right"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="parameter" use="required">
							<xs:annotation>
								<xs:documentation>Function-specific parameter value.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:hexBinary">
									<xs:minLength value="1"/>
									<xs:maxLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="offset" default="0">
							<xs:annotation>
								<xs:documentation>Byte offset into the message of the byte to modify.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedInt">
									<xs:minInclusive value="0"/>
									<xs:maxInclusive value="65535"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="a615aLoadableTargets">
		<xs:complexType>
