											<xs:element ref="RxSapUdpReferenceName"/>
										</xs:choice>
										<xs:choice>
											<xs:element ref="TxSapUdpReferenceId"/>
											<xs:element ref="TxSapUdpReferenceName"/>
										</xs:choice>
