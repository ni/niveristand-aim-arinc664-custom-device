<?xml version="1.0" encoding="UTF-8"?>
<!-- version 1.0 from Fabio Mussi (fabio.mussi@ni.com) -->
<xs:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:avif="http://www.aviftech.com/Schemas/Databus/" targetNamespace="http://www.aviftech.com/Schemas/Databus/" xmlns="http://www.aviftech.com/Schemas/Databus/" >
  <xs:attributeGroup name="portGroup">
    <xs:attribute name="sessionType" use="required">
      <xs:annotation>
        <xs:documentation>The session type for the port (Tx-Generic, VL oriented, or Monitor)</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value ="Tx-Generic" />
          <xs:enumeration value ="Tx-UdpPortOriented" />
          <xs:enumeration value ="Rx-VlOriented" />
          <xs:enumeration value ="Rx-Monitor" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="RxModeControlInGroup">
    <xs:attribute name="ReceiveMode" default="FDX_RX_CHRONO">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value ="FDX_RX_CHRONO" />
          <xs:enumeration value ="FDX_RX_VL" />
        </xs:restriction>
      </xs:simpleType>      
    </xs:attribute>
  </xs:attributeGroup>
  <xs:complexType name="boardType">
    <xs:annotation>
      <xs:documentation>Represents the physical device with multiple ports.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="boardConfiguration" type="boardConfigurationType" minOccurs="0" />
			<xs:element name="port" type="portType" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="boardConfigurationType" >
    <xs:all >
      <xs:element name="boardId" default="0" type="xs:int" >
        <xs:annotation>
          <xs:documentation>The ID of the board. It is a zero index element to enumerate the AFDX boards in the system.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="portType">
    <xs:annotation>
      <xs:documentation>Represents the physical port of a device.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="portConfiguration" type="portConfigurationType" minOccurs="0" />
      <xs:choice>
        <xs:element ref="TxGenericSession"/>
        <xs:element ref="RxMonitorSession"/>
      </xs:choice>
    </xs:sequence>
    <xs:attributeGroup ref ="portGroup" />
  </xs:complexType>
  <xs:complexType name="portConfigurationType" >
    <xs:all minOccurs="0">
      <xs:element name="portId" default="0" type="xs:int" >
        <xs:annotation>
          <xs:documentation>The ID of the board. It is a zero index element to enumerate the AFDX boards in the system.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="portSpeed" default="FDX_100MBIT" >
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value ="FDX_100MBIT" />
            <xs:enumeration value ="FDX_10MBIT" />
            <xs:enumeration value ="FDX_AUTO_100MBIT" />
            <xs:enumeration value ="FDX_AUTO_10MBIT" />
            <xs:enumeration value ="FDX_AUTO_ERROR" />
            <xs:enumeration value ="FDX_1000MBIT" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="portConfig" default="FDX_SINGLE" >
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value ="FDX_SINGLE" />
            <xs:enumeration value ="FDX_REDUNDANT" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="TxGenericSessionType">
    <xs:annotation>
      <xs:documentation>The session type for Generic Transmit</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name ="txQueueConfiguration" type="txQueueConfigurationType" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="txQueueConfigurationType">
    <xs:all minOccurs="0">
      <xs:element name="QueueSizeIn" default="0" type="xs:int" >
        <xs:annotation>
          <xs:documentation>The size of the transmit queue. If is zero, internal queue size will be used</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>    
  </xs:complexType>
  <xs:complexType name="RxMonitorSessionType">
    <xs:annotation>
      <xs:documentation>The session type for Chronological Monitor.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name ="monitorConfiguration" type="monitorConfigurationType" minOccurs="0" />
      <xs:element name ="rxQueueConfiguration" type="rxQueueConfigurationType" minOccurs="0" />
    </xs:sequence>
    <xs:attributeGroup ref="RxModeControlInGroup"/>
  </xs:complexType>
  <xs:complexType name="monitorConfigurationType">
    <xs:all minOccurs="0">
      <xs:element name="DefaultCronoMode" default="FDX_RX_DEFAULT_ENA_CNT" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The size of the transmit queue. If is zero, internal queue size will be used</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value ="FDX_RX_DEFAULT_ENA_CNT" />
            <xs:enumeration value ="FDX_RX_DEFAULT_MON_ENA_ALL" />
            <xs:enumeration value ="FDX_RX_DEFAULT_MON_ENA_GOOD" />           
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="GlbMonBufferSizeIn" default="0" type="xs:int" minOccurs="0" />     
    </xs:all>
  </xs:complexType>
  <xs:complexType name="rxQueueConfigurationType">
    <xs:all>
      <xs:element name="TriggerPosition" default="0" type="xs:int" />
      <xs:element name="MaxFileSizeMB" default="1" type="xs:int"  />
      <xs:element name="CaptureMode" default="FDX_MON_SINGLE" >
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value ="FDX_MON_SINGLE" />
            <xs:enumeration value ="FDX_MON_CONTINUOUS" />
            <xs:enumeration value ="FDX_MON_SELECTIVE" />
            <xs:enumeration value ="FDX_MON_RECORDING" />
            <xs:enumeration value ="FDX_MON_REROS" />
            <xs:enumeration value ="FDX_MON_HMU" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Strobe" default="FDX_MON_STROBE_DIS" >
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value ="FDX_MON_STROBE_DIS" />
            <xs:enumeration value ="FDX_MON_STROBE_STOP" />
            <xs:enumeration value ="FDX_MON_STROBE_START" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:element name="board" type="boardType" />
  <xs:element name="port" type="portType" />
  <xs:element name="TxGenericSession" type="TxGenericSessionType" />
  <xs:element name="RxMonitorSession" type="RxMonitorSessionType" />
  <xs:element name="ethSystem">
		<xs:annotation>
			<xs:documentation>Container for a whole system configuration for all Ethernet/A664 Devices in a host system</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="f16HsnDevice" maxOccurs="32"/>
				</xs:sequence>
				<xs:sequence>
					<xs:element ref="hostLan"/>
					<xs:choice minOccurs="0">
						<xs:element ref="a664Device" maxOccurs="32"/>
						<xs:element ref="edeDevice" maxOccurs="32"/>
					</xs:choice>
				</xs:sequence>
				<xs:sequence>
					<xs:choice>
						<xs:element ref="a664Device" maxOccurs="32"/>
						<xs:element ref="edeDevice" maxOccurs="32"/>
					</xs:choice>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="hostLan">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="LanTftp" minOccurs="0">
					<xs:complexType>
						<xs:choice>
							<xs:sequence>
								<xs:element ref="LanTftpClient" maxOccurs="unbounded"/>
								<xs:element ref="LanTftpServer" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
							<xs:element ref="LanTftpServer" maxOccurs="unbounded"/>
						</xs:choice>
					</xs:complexType>
					<xs:unique name="UniqueClientId">
						<xs:selector xpath="avif:LanTftpClient"/>
						<xs:field xpath="@id"/>
					</xs:unique>
					<xs:unique name="UniqueClientName">
						<xs:selector xpath="avif:LanTftpClient"/>
						<xs:field xpath="@name"/>
					</xs:unique>
					<xs:unique name="UniqueServerId">
						<xs:selector xpath="avif:LanTftpServer"/>
						<xs:field xpath="@id"/>
					</xs:unique>
					<xs:unique name="UniqueServerName">
						<xs:selector xpath="avif:LanTftpServer"/>
						<xs:field xpath="@name"/>
					</xs:unique>
				</xs:element>
				<xs:element name="a615aTargets" minOccurs="0">
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element ref="a615aTargetConnection">
								<xs:annotation>
									<xs:documentation>The DataLoader side connection to an ARINC 615A target.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
					<xs:unique name="UniqueClientIdReference">
						<xs:selector xpath="avif:a615aTargetConnection"/>
						<xs:field xpath="@id"/>
					</xs:unique>
					<xs:unique name="UniqueClientNameReference">
						<xs:selector xpath="avif:a615aTargetConnection"/>
						<xs:field xpath="@name"/>
					</xs:unique>
				</xs:element>
				<xs:element name="a615aLoadableTargets" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="a615aLoadableTarget" maxOccurs="32">
								<xs:annotation>
									<xs:documentation>The ARINC-615A target side configuration.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="a615aLoaderConnection"/>
									</xs:sequence>
									<xs:attribute name="a615aFindPort" default="1001">
										<xs:simpleType>
											<xs:restriction base="xs:unsignedShort">
												<xs:minInclusive value="0"/>
												<xs:maxInclusive value="65535"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
					<xs:unique name="a615a664LoaderConnectionIdUnique">
						<xs:selector xpath="avif:a615aLoadableTarget/a615aLoaderConnection"/>
						<xs:field xpath="@id"/>
					</xs:unique>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="nameGroup">
		<xs:annotation>
			<xs:documentation>Generic Attribute to hold the name of objects</xs:documentation>
		</xs:annotation>
		<xs:attribute name="name" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:whiteSpace value="preserve"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:element name="f16HsnSubSystem">
		<xs:annotation>
			<xs:documentation>A Simulated F-16 HSN Subsystem</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MacAddress"/>
				<xs:element name="hsnOutputs" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="hsnRtpTxPort" maxOccurs="4096">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="source">
											<xs:complexType>
												<xs:attributeGroup ref="UdpPortGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="destination">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="MacAddress" minOccurs="0"/>
												</xs:sequence>
												<xs:attributeGroup ref="IpAddressGroup"/>
												<xs:attributeGroup ref="UdpPortGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="hsnMessage" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>The total number of messages over the entire device is limited to 4096.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence minOccurs="0">
													<xs:element name="DataBuffer">
														<xs:simpleType>
															<xs:restriction base="xs:hexBinary">
																<xs:minLength value="1"/>
																<xs:maxLength value="1440"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element ref="DynamicData" minOccurs="0"/>
												</xs:sequence>
												<xs:attribute name="uid" use="required">
													<xs:annotation>
														<xs:documentation>A unique message identifier. The max number of unique messages over the entire device is 4096.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:unsignedInt">
															<xs:minInclusive value="0"/>
															<xs:maxInclusive value="4095"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attributeGroup ref="nameGroup"/>
												<xs:attribute name="msgID" use="required">
													<xs:annotation>
														<xs:documentation>The message id.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:hexBinary">
															<xs:minLength value="1"/>
															<xs:maxLength value="4"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="msgRate" use="required">
													<xs:annotation>
														<xs:documentation>The frequency at which to send the message.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="50Hz"/>
															<xs:enumeration value="25Hz"/>
															<xs:enumeration value="12.5Hz"/>
															<xs:enumeration value="6.25Hz"/>
															<xs:enumeration value="3.125Hz"/>
															<xs:enumeration value="1.5625Hz"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="msgSize" use="required">
													<xs:annotation>
														<xs:documentation>The message size in bytes.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:integer">
															<xs:minInclusive value="1"/>
															<xs:maxInclusive value="1440"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="msgsPerFrame" type="xs:int" default="1">
													<xs:annotation>
														<xs:documentation>Number of copies of the message in a minor frame. This is a rate multiplier for the message. Default value is 1</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attributeGroup ref="nameGroup"/>
									<xs:attributeGroup ref="TxMessagePortIDgroup"/>
									<xs:attribute name="did" use="required">
										<xs:annotation>
											<xs:documentation>Destination Controller ID</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:hexBinary">
												<xs:minLength value="1"/>
												<xs:maxLength value="4"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="isCrcEnabled">
										<xs:annotation>
											<xs:documentation>Enable/disable VlRtp CRC and UDP checksum calculation</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attributeGroup ref="nameGroup"/>
			<xs:attribute name="subSystem" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The subsystem identifier</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="cid" use="required">
				<xs:annotation>
					<xs:documentation>The Controller ID (CID) for the simulated Subsystem. Unique over entire device.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:hexBinary">
						<xs:minLength value="1"/>
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="IpAddressGroup"/>
			<xs:attributeGroup ref="a664NetworkGroup"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="f16HsnDevice">
		<xs:annotation>
			<xs:documentation>An F-16 HSN Simulator Device</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="f16HsnSubSystem" minOccurs="0" maxOccurs="31"/>
			</xs:sequence>
			<xs:attributeGroup ref="nameGroup"/>
			<xs:attributeGroup ref="NetworkSpeedGroup"/>
		</xs:complexType>
		<xs:unique name="TxPortIdUnique">
			<xs:selector xpath="avif:f16HsnSubSystem/avif:hsnOutputs/avif:hsnRtpTxPort"/>
			<xs:field xpath="@portId"/>
		</xs:unique>
		<xs:unique name="MessageIdUnique">
			<xs:selector xpath="avif:f16HsnSubSystem/avif:hsnOutputs/avif:hsnRtpTxPort/avif:hsnMessage"/>
			<xs:field xpath="@uid"/>
		</xs:unique>
		<xs:unique name="SubSystemIdUnique">
			<xs:selector xpath="avif:f16HsnSubSystem"/>
			<xs:field xpath="@cid"/>
			<xs:field xpath="@IpAddress"/>
		</xs:unique>
	</xs:element>
	<xs:element name="a664Device">
		<xs:annotation>
			<xs:documentation>An ARINC664 Hardware Device</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element name="cotsInputs" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="cotsMacPort" maxOccurs="32"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="cotsOutputs" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="TxCotsMacPort" maxOccurs="2"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element ref="a664EndSystem" minOccurs="0" maxOccurs="127"/>
				</xs:sequence>
				<xs:element name="ethernetTester">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="testerInputs" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="cotsMacPort" maxOccurs="32"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="testerOutputs" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="TxCotsMacRawPort" maxOccurs="2"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:attributeGroup ref="nameGroup"/>
			<xs:attributeGroup ref="NetworkSpeedGroup"/>
			<xs:attributeGroup ref="a664MacGroupAddressConstantGroup">
				<xs:annotation>
					<xs:documentation>Specifier of the contant field used in the first 4 bytes of MAC Destination (Group) addresses of ARINC664 frames. The VLid composes the remaing 2 bytes of the address. If this attribute is NOT specified a default value of 03:00:00:00 will be assumed.</xs:documentation>
				</xs:annotation>
			</xs:attributeGroup>
			<xs:attribute name="a664PsnRange" use="optional" default="2">
				<xs:annotation>
					<xs:documentation>The Previous Sequence Number Range to be used by the Redundancy Management algorithm (per A664 P7) if RM is enabled on an input VL. If this optional attribute is NOT specified then a default value of 2 is assumed (in accordance with A664 p7).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
		<xs:unique name="a615aTargetConnectionUnique">
			<xs:selector xpath="avif:a664EndSystem/avif:a615aTargets/avif:a615aTargetConnection"/>
			<xs:field xpath="@id"/>
		</xs:unique>
		<xs:unique name="UniqueOutputVLId">
			<xs:selector xpath="avif:a664EndSystem/avif:a664Outputs/avif:a664EsOutputVL"/>
			<xs:field xpath="@VlId"/>
		</xs:unique>
		<xs:unique name="UniqueInputMessagePortId">
			<xs:selector xpath="avif:a664EndSystem/avif:a664Inputs/avif:a664EsInputVL/avif:a664InputVL/avif:RxMessagePort"/>
			<xs:field xpath="@portId"/>
		</xs:unique>
		<xs:unique name="UniqueOutputMessagePortId">
			<xs:selector xpath="avif:a664EndSystem/avif:a664Outputs/avif:a664EsOutputVL/avif:a664OutputVL/avif:a664SubVL/avif:TxMessagePort"/>
			<xs:field xpath="@portId"/>
		</xs:unique>
		<xs:unique name="UniqueSubVLUID">
			<xs:selector xpath="avif:a664EndSystem/avif:a664Outputs/avif:a664EsOutputVL/avif:a664OutputVL/avif:a664SubVL"/>
			<xs:field xpath="@subVlUID"/>
		</xs:unique>
		<xs:unique name="CotsRxMessagePortUID">
			<xs:selector xpath="avif:cotsInputs/avif:cotsMacPort"/>
			<xs:field xpath="@portId"/>
		</xs:unique>
		<xs:unique name="InputVLUID">
			<xs:selector xpath="avif:a664EndSystem/avif:a664Inputs/avif:a664EsInputVL"/>
			<xs:field xpath="@inputVLUID"/>
		</xs:unique>
		<xs:unique name="CotsTxMessagePortUID">
			<xs:selector xpath="avif:cotsOutputs/avif:TxCotsMacPort"/>
			<xs:field xpath="@portId"/>
		</xs:unique>
		<xs:unique name="EndSystemUniqueId">
			<xs:selector xpath="avif:a664EndSystem"/>
			<xs:field xpath="@id"/>
		</xs:unique>
		<xs:unique name="testerTxCotsMacRawPortID">
			<xs:selector xpath="avif:ethernetTester/avif:testerOutputs/TxCotsMacRawPort"/>
			<xs:field xpath="@portId"/>
		</xs:unique>
		<xs:unique name="testerRxCotsMacPortID">
			<xs:selector xpath="avif:ethernetTester/avif:testerInputs/cotsMacPort"/>
			<xs:field xpath="@portId"/>
