	<xs:element name="LoadList">
		<xs:annotation>
			<xs:documentation>The top-level load container</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="LoadInfo" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>A Load definition</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="hwid_pos" use="required">
							<xs:annotation>
								<xs:documentation>The identifier for the associated target configuration. This value can be repeated, but must reference the unique HwId and Position of the target configuration in the format THWID or THWId_Pos (underscrore separating id and position).</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value="[a-zA-Z0-9_-]{1,15}_[a-zA-Z0-9_-]{1,8}"/>
									<xs:pattern value="[a-zA-Z0-9_-]{1,15}"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="priority" use="required">
							<xs:annotation>
								<xs:documentation>The load priority. Loads are processed from the lowest priority first, where multiple loads with the same priority are performed simultaneously. Note that priority must be different for any target (hardware id + position) that appears more than once.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="media-file" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>The absolute path to the referenced media file (*.LUM/*.LUH)</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="retryCount" type="xs:integer" use="optional" default="-1">
							<xs:annotation>
								<xs:documentation>The number of times to retry the load before aborting. Default: -1 (continuous)</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="targetDelay" type="xs:int" use="optional" default="0">
							<xs:annotation>
								<xs:documentation>The delay (in seconds) after this load before moving to the next priority load of the same target. This value is ignored if useGlobalParity is true. Default: 0</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="PriorityDelay" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>The delay in seconds after all loads at the specified priority have completed before moving to the next priority. The next priority load will begin immediately if not specified. This will be ignored if useGlobalPriority is false.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:int">
								<xs:attribute name="priority" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:integer">
											<xs:minInclusive value="1"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="useGlobalPriority" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>If true, the PriorityDelay settings will be utilized. Otherwise, the per LoadInfo delay's will be utilized. Default: false</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
		<xs:unique name="UniquePriority">
			<xs:selector xpath="avif:LoadInfo"/>
			<xs:field xpath="@hwid_pos"/>
			<xs:field xpath="@priority"/>
		</xs:unique>
		<xs:unique name="UniqueDelayPriority">
			<xs:selector xpath="avif:PriorityDelay"/>
			<xs:field xpath="@priority"/>
		</xs:unique>
	</xs:element>
